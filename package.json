{
  "name": "map",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:preview": "npx wrangler pages dev .vercel/output/static --d1=DB --r2=BUCKET --kv=CACHE",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:migrate": "npx wrangler d1 execute map-db --local --file=drizzle/migrations/0001_init.sql && npx wrangler d1 execute map-db --local --file=drizzle/migrations/0002_map_pins.sql && npx wrangler d1 execute map-db --local --file=drizzle/migrations/0003_email_verification.sql",
    "db:migrate:prod": "npx wrangler d1 execute map-db --file=drizzle/migrations/0001_init.sql && npx wrangler d1 execute map-db --file=drizzle/migrations/0002_map_pins.sql && npx wrangler d1 execute map-db --file=drizzle/migrations/0003_email_verification.sql",
    "deploy": "pnpm pages:build && npx wrangler pages deploy .vercel/output/static --project-name=map",
    "deploy:workers": "cd cf-workers && npx wrangler deploy"
  },
  "dependencies": {
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "lucia": "^3.2.0",
    "@lucia-auth/adapter-sqlite": "^3.0.2",
    "maplibre-gl": "^4.7.0",
    "zustand": "^5.0.0",
    "drizzle-orm": "^0.38.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "class-variance-authority": "^0.7.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "lucide-react": "^0.468.0",
    "rbush": "^4.0.1",
    "idb-keyval": "^6.2.1",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.0",
    "@cloudflare/workers-types": "^4.20250109.0",
    "wrangler": "^3.99.0",
    "drizzle-kit": "^0.30.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/rbush": "^4.0.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.0",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "postcss": "^8.5.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "@eslint/eslintrc": "^3.0.0"
  }
}
